{
  "name": "ember-cli-global-env",
  "version": "1.0.0",
  "description": "support global env const in js and hbs for ember app",
  "keywords": [
    "ember-addon",
    "babel-plugin",
    "template-plugin"
  ],
  "repository": "https://github.com/houfeng0923/ember-cli-global-env",
  "license": "MIT",
  "author": "houfeng0923@gmail.com",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm run test:babel && npm run test:hbs",
    "test:babel": "jest",
    "test:hbs": "APP_BRAND=cfx ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "babel-plugin-tester": "^9.0.1",
    "debug": "^4.1.1",
    "ember-cli-babel": "^7.17.2",
    "ember-cli-babel-plugin-helpers": "^1.1.0",
    "ember-cli-htmlbars": "^4.2.2",
    "ember-template-lint": "^2.4.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.12.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.12.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint-plugin-ember": "^7.7.2",
    "eslint-plugin-node": "^11.0.0",
    "jest": "^25.4.0",
    "loader.js": "^4.7.0",
    "qunit-dom": "^1.0.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": "ember-cli-htmlbars"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "fixtures",
      ".node_modules.ember-try"
    ]
  }
}
